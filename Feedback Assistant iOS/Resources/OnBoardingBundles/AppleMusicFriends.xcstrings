    "SECOND_BULLET": {
      "extractionState": "manual",
      "localizations": {
        "en_IN": {
          "stringUnit": {
            "state": "translated",
            "value": "Apple Music can recommend new friends by periodically checking the contacts on your device. This is done in such a way that Apple Music does not learn your contacts."
