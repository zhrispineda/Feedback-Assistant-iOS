    "SPLASH_SUMMARY_WIFI": {
      "extractionState": "manual",
      "localizations": {
        "en_IN": {
          "stringUnit": {
            "state": "translated",
            "value": "Wi-Fi Calling is designed to protect your information and enable you to choose what you share."
          }
        },
        "ko": {
          "stringUnit": {
            "state": "translated",
            "value": "Wi‑Fi 통화는 사용자의 정보를 보호하도록 설계되었으며 공유할 내용을 사용자가 선택할 수 있습니다."
          }
        },
        "he": {
          "stringUnit": {
            "state": "translated",
            "value": "״שיחות דרך רשת אלחוטית״ תוכנן באופן שמגן על המידע שלך ומאפשר לך לבחור מה לשתף ומה לא לשתף."
          }
        },
        "id": {
          "stringUnit": {
            "state": "translated",
            "value": "Panggilan Wi-Fi dirancang untuk melindungi informasi Anda dan memungkinkan Anda untuk memilih apa yang Anda bagikan."
          }
        },
        "nl": {
          "stringUnit": {
            "state": "translated",
            "value": "'Bellen via wifi' is ontworpen om je gegevens te beschermen en om je te laten bepalen wat je deelt."
          }
        },
        "tr": {
          "stringUnit": {
            "state": "translated",
            "value": "Wi-Fi ile Arama, bilgilerinizi koruyacak ve neyi paylaşacağınızı seçmenizi sağlayacak şekilde tasarlanmıştır."
          }
        },
        "fi": {
          "stringUnit": {
            "state": "translated",
            "value": "Wi-Fi-puhelut on suunniteltu niin, että tietosi ovat turvassa. Voit itse valita, mitä haluat jakaa."
          }
        },
        "hu": {
          "stringUnit": {
            "state": "translated",
            "value": "A Wi-Fi-hívást úgy tervezték, hogy megvédje az Ön információit, és lehetővé tegye, hogy Ön kiválassza, mit oszt meg."
          }
        },
        "ru": {
          "stringUnit": {
            "state": "translated",
            "value": "Ваши данные, используемые функцией «Вызовы по Wi‑Fi», надежно защищены, и только Вы решаете, чем поделиться."
          }
        },
        "it": {
          "stringUnit": {
            "state": "translated",
            "value": "L’opzione “Chiamate Wi-Fi” è concepita in modo da proteggere le tue informazioni e consentirti di scegliere i contenuti da condividere."
          }
        },
        "es_419": {
          "stringUnit": {
            "state": "translated",
            "value": "Llamadas por Wi-Fi se diseñó para proteger tu información y permitirte elegir lo que compartes."
          }
        },
        "el": {
          "stringUnit": {
            "state": "translated",
